// Place your settings in this file to overwrite the default settings
{
    "editor.fontFamily": "'Operator Mono', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.wordWrap": "off",
    "editor.minimap.enabled": false,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "files.exclude": {
        "**/*.pyc": true,
        "**/*.a": true,
        "**/*.class": true,
        "**/*.db": true,
        "**/*.dll": true,
        "**/*.dylib": true,
        "**/*.egg-info": true,
        "**/*.exe": true,
        "**/*.idb": true,
        "**/*.lib": true,
        "**/*.ncb": true,
        "**/*.o": true,
        "**/*.obj": true,
        "**/*.pdb": true,
        "**/*.psd": true,
        "**/*.pyo": true,
        "**/*.sdf": true,
        "**/*.so": true,
        "**/*.sublime-workspace": true,
        "**/*.suo": true,
        "**/.bundle": true,
        "**/.coverage": true,
        "**/.DS_Store": true,
        "**/.expo": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.pygments-cache": true,
        "**/.sass-cache": true,
        "**/.svn": true,
        "**/.tox": true,
        "**/.vagrant": true,
        "**/.venv": true,
        "**/.virtualenv": true,
        "**/.vscode": true,
        "**/__pycache__": true,
        "**/_build": true,
        "**/_deploy": true,
        "**/_site": true,
        "**/backstop_data": true,
        "**/build": true,
        "**/coverage": true,
        "**/coverage_html_report": true,
        "**/css_regression_data": true,
        "**/CVS": true,
        "**/db.sqlite3": true,
        "**/dist": true,
        "**/env": true,
        "**/es5": true,
        "**/es": true,
        "**/logs*": true,
        "**/node_modules*": true,
        "**/npm-debug.log*": true,
        "**/npm-shrinkwrap.json": true,
        "**/package-lock.json": true,
        "**/core/static/css/*": true,
        "**/core/static/js/*": true,
        "**/static/wagtailadmin": true,
        "**/tmp": true,
        "**/var": true,
        "**/venv": true,
        "**/www": true,
        "**/webpack-stats.html": true,
        "**/yarn.lock": true
    },
    "window.zoomLevel": 0,
    "terminal.external.osxExec": "iTerm.app",
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "markdownlint.config": {
        "MD003": false,
        "MD013": false,
        "MD024": false
    },
    "workbench.editor.enablePreview": false,
    "workbench.editor.openPositioning": "right",
    "workbench.sideBar.location": "left",
    "html.format.enable": false,
    "html.format.wrapLineLength": 0,
    "html.format.extraLiners": "object",
    "html.suggest.angular1": false,
    "html.suggest.ionic": false,
    "editor.tabSize": 4,
    "git.enabled": true,
    "git.autorefresh": true,
    "git.autofetch": true,
    "git.countBadge": "all",
    "css.validate": true,
    "scss.validate": true,
    "workbench.iconTheme": "seti",
    "typescript.check.npmIsInstalled": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Monokai",
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "all",
    "prettier.singleQuote": true
}
