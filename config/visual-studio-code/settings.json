// Place your settings in this file to overwrite the default settings
{
  "editor.fontFamily": "'Fira Code', 'Operator Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": "'ss02', 'ss19'",
  "editor.fontSize": 16,
  "editor.wordWrap": "off",
  "editor.minimap.enabled": false,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "files.exclude": {
    "**/*.pyc": true,
    "**/*.a": true,
    "**/*.bundle*": true,
    "**/*.bundle.js": true,
    "**/*.class": true,
    "**/*.cache": true,
    "**/*.db": true,
    "**/*.dll": true,
    "**/*.dylib": true,
    "**/*.egg-info": true,
    "**/*.exe": true,
    "**/*.idb": true,
    "**/*.lib": true,
    "**/*.log": true,
    "**/*.map": true,
    "**/*.ncb": true,
    "**/*.o": true,
    "**/*.obj": true,
    "**/*.pdb": true,
    "**/*.psd": true,
    "**/*.pyo": true,
    "**/*.sdf": true,
    "**/*.so": true,
    "**/*.sublime-workspace": true,
    "**/*.suo": true,
    "**/.coverage": true,
    "**/.DS_Store": true,
    "**/.expo": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.pygments-cache": true,
    "**/.sass-cache": true,
    "**/.svn": true,
    "**/.tmp": true,
    "**/.tox": true,
    "**/.vagrant": true,
    "**/.venv": true,
    "**/.virtualenv": true,
    "**/.vscode": true,
    "**/__pycache__": true,
    "**/_book": true,
    "**/_build": true,
    "**/_deploy": true,
    "**/_site": true,
    "**/build": true,
    "**/coverage": true,
    "**/coverage_html_report": true,
    "**/css_regression_data": true,
    "**/CVS": true,
    "**/db.sqlite3": true,
    "**/dist": true,
    "**/env": true,
    "**/es5": true,
    "**/es": true,
    "**/logs*": true,
    "**/node_modules*": true,
    "**/npm-debug.log*": true,
    "**/npm-shrinkwrap.json": true,
    "**/package-lock.json": true,
    "**/source-map-explorer.html": true,
    "**/static/wagtailadmin": true,
    "**/static_compiled": true,
    "media": true,
    "**/tmp": true,
    "**/var": true,
    "**/venv": true,
    "**/vendor": true,
    "**/www": true,
    "**/webpack-stats.html": true,
    "**/yarn.lock": true
  },
  "window.zoomLevel": 0,
  "terminal.external.osxExec": "iTerm.app",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": false,
  "python.linting.pylintEnabled": false,
  "javascript.validate.enable": true,
  "markdownlint.config": {
    "MD003": false,
    "MD013": false,
    "MD024": false,
    "MD041": false,
    "MD029": false
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.openPositioning": "right",
  "workbench.sideBar.location": "left",
  "html.format.enable": false,
  "html.format.wrapLineLength": 0,
  "html.format.extraLiners": "object",
  "editor.tabSize": 4,
  "git.enabled": true,
  "git.autorefresh": true,
  "git.autofetch": true,
  "git.countBadge": "all",
  "css.validate": true,
  "scss.validate": true,
  "workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
  "typescript.check.npmIsInstalled": false,
  "workbench.startupEditor": "newUntitledFile",
  "files.associations": {
    "*.j2": "html",
    "*.html": "html",
    "**/templates/**/*.html": "html",
    "Vagrantfile": "ruby"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": false,
  "workbench.statusBar.feedback.visible": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": false,
  "editor.accessibilitySupport": "off",
  "flow.useNPMPackagedFlow": true,
  "htmlhint.options": {
    "doctype-first": false
  },
  "explorer.confirmDelete": false,
  // "javascript.implicitProjectConfig.checkJs": true,
  "workbench.settings.editor": "json",
  "workbench.iconTheme": "material-icon-theme",
  "python.formatting.provider": "black",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": false
}
