// Place your settings in this file to overwrite the default settings
{
  "workbench.settings.editor": "json",
  "editor.fontFamily": "'Fira Code', 'Operator Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": "'ss02', 'ss19'",
  "editor.fontSize": 16,
  "editor.wordWrap": "off",
  "editor.minimap.enabled": false,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.tabSize": 4,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "seti",
  "files.exclude": {
    "**/*.pyc": true,
    "**/*.a": true,
    "**/*.bundle*": true,
    "**/*.bundle.js": true,
    "**/*.class": true,
    "**/*.cache": true,
    "**/*.db": true,
    "**/*.dll": true,
    "**/*.dylib": true,
    "**/*.egg-info": true,
    "**/*.exe": true,
    "**/*.idb": true,
    "**/*.lib": true,
    "**/*.log": true,
    "**/*.map": true,
    "**/*.ncb": true,
    "**/*.o": true,
    "**/*.obj": true,
    "**/*.pdb": true,
    "**/*.psd": true,
    "**/*.pyo": true,
    "**/*.sdf": true,
    "**/*.so": true,
    "**/*.sublime-workspace": true,
    "**/*.suo": true,
    "**/*.vsix": true,
    "**/.coverage": true,
    "**/.docusaurus": true,
    "**/.DS_Store": true,
    "**/.expo": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.jekyll-cache": true,
    "**/.next": true,
    "**/.nyc_output": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.pygments-cache": true,
    "**/.sass-cache": true,
    "**/.svn": true,
    "**/.tmp": true,
    "**/.tox": true,
    "**/.vagrant": true,
    "**/.venv": true,
    "**/.virtualenv": true,
    "**/.vscode": true,
    "**/.vscode-test": true,
    "**/__pycache__": true,
    "**/_book": true,
    "**/_build": true,
    "**/_deploy": true,
    "**/_site": true,
    "**/build": true,
    "**/coverage": true,
    "**/coverage_html_report": true,
    "**/css_regression_data": true,
    "**/CVS": true,
    "**/db.sqlite3": true,
    "**/dist": true,
    "**/env": true,
    "**/es5": true,
    "**/es": true,
    "**/logs*": true,
    "**/node_modules*": true,
    "**/npm-debug.log*": true,
    "**/npm-shrinkwrap.json": true,
    "**/package-lock.json": true,
    "**/poetry.lock": true,
    "**/source-map-explorer.html": true,
    "**/static/wagtailadmin": true,
    "**/static_compiled": true,
    "**/storybook-static": true,
    "media": true,
    "target": true,
    "**/tmp": true,
    "**/var": true,
    "**/venv": true,
    "**/vendor": true,
    "**/www": true,
    "**/webpack-stats.html": true,
    "**/yarn.lock": true
  },
  "window.zoomLevel": 1,
  "terminal.external.osxExec": "iTerm.app",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "python.linting.flake8Enabled": true,
  "python.linting.pylintEnabled": false,
  "python.linting.mypyEnabled": false,
  "python.formatting.provider": "black",
  "python.jediEnabled": false,
  "javascript.validate.enable": true,
  "markdownlint.config": {
    "MD003": false,
    "MD013": false,
    "MD024": false,
    "MD041": false,
    "MD029": false
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.openPositioning": "right",
  "workbench.sideBar.location": "left",
  "html.format.enable": false,
  "html.format.wrapLineLength": 0,
  "html.format.extraLiners": "object",
  "git.enabled": true,
  "git.autorefresh": true,
  "git.autofetch": true,
  "git.countBadge": "all",
  "css.validate": true,
  "scss.validate": true,
  "workbench.colorTheme": "Monokai Pro (Filter Machine)",
  "typescript.check.npmIsInstalled": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": false,
  "editor.accessibilitySupport": "off",
  "breadcrumbs.enabled": true,
  "explorer.confirmDelete": false,
  "editor.suggestSelection": "first",
  "terminal.integrated.rendererType": "experimentalWebgl",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "flow.useNPMPackagedFlow": true,
  "python.languageServer": "Microsoft",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": true
  },
  "files.associations": {
    "*.j2": "twig",
    "**/templates/**/*.html": "twig",
    "*.njk": "twig",
    "*.liquid": "twig",
    "Vagrantfile": "ruby",
    ".coveragerc": "ini",
    "*.in": "ini",
    ".pyre_configuration": "json",
    ".{aliases,exports,path,extra,functions}": "shellscript"
  },
  "emmet.includeLanguages": {
    "django-html": "html",
    "twig": "html",
    "javascript": "html"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "tailwindCSS.includeLanguages": {
    "Vue": "html",
    "scss": "css"
  },
  "tailwindCSS.emmetCompletions": true
}
